<div class="row">
  <div class="col-12 col-lg-9">
    <%= f.input :nome, input_html: { class: "keyup", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12 col-lg-3">
    <%= f.input :inep, input_html: { class: "keyup", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12">
    <%= f.input :endereco, input_html: { class: "keyup", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12 col-lg-3">
    <%= f.input :duracao_aula, input_html: { class: "keyup", type: "time", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12 col-lg-3">
    <%= f.input :horario_inicio_aula, input_html: { class: "keyup", type: "time", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12 col-lg-3">
    <%= f.input :horario_fim_aulas, input_html: { class: "keyup", type: "time", disabled: local_assigns[:disabled] } %>
  </div>
  <div class="col-12 col-lg-3">
    <%= f.input :tempos_aula, input_html: { class: "keyup", disabled: local_assigns[:disabled] } %>
  </div>
</div>
<script>
  $(document).ready(function(){
   $('input.timepicker').mask("00:00", {placeholder: "HH:MM"});
  });
</script>